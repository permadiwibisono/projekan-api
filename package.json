{
  "name": "projekan-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/bin/server.js",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && babel ./src --out-dir dist --copy-files",
    "dev": "cross-env DEBUG=app:* nodemon --watch ./src --exec babel-node ./src/bin/server.js",
    "start": "cross-env DEBUG=app:startup,app:log,app:error NODE_ENV=production node ./dist/bin/server.js",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\""
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.md": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/permadiwibisono/projekan-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/permadiwibisono/projekan-api/issues"
  },
  "homepage": "https://github.com/permadiwibisono/projekan-api#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "http-errors": "^1.8.0",
    "joi": "^17.4.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2"
  }
}
